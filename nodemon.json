{
	"restartable": "rs",
	"ignore": [
	  ".git",
	  "node_modules/**/node_modules",
	  "frontend/node_modules",
	  "frontend/build"
	],
	"verbose": true,
	"events": {
	  "start": "npm run build --prefix ./frontend"
	},
	"watch": [
	  "frontend",
	  "src"
	],
	"env": {
	  "NODE_ENV": "development"
	},
	"ext": "js,json,tsx,ts"
  }